{
  "name": "@aligntrue/exporters",
  "version": "0.1.1",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./cursor": {
      "types": "./dist/cursor/index.d.ts",
      "default": "./dist/cursor/index.js"
    },
    "./agents-md": {
      "types": "./dist/agents-md/index.d.ts",
      "default": "./dist/agents-md/index.js"
    },
    "./vscode-mcp": {
      "types": "./dist/vscode-mcp/index.d.ts",
      "default": "./dist/vscode-mcp/index.js"
    },
    "./utils/section-parser": {
      "types": "./dist/utils/section-parser.d.ts",
      "default": "./dist/utils/section-parser.js"
    }
  },
  "scripts": {
    "build": "tsc && node -e \"const fs=require('fs'),path=require('path');function copyManifests(dir){fs.readdirSync(dir,{withFileTypes:true}).forEach(e=>{const p=path.join(dir,e.name);if(e.isDirectory())copyManifests(p);else if(e.name==='manifest.json'){const t=p.replace('src','dist');fs.mkdirSync(path.dirname(t),{recursive:true});const content=fs.readFileSync(p,'utf8');const fixed=content.replace(/\\\"\\.\\/index\\.ts\\\"/g,'\\\"./index.js\\\"');fs.writeFileSync(t,fixed)}})};copyManifests('src')\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aligntrue/core": "^0.1.0",
    "@aligntrue/file-utils": "^0.1.0",
    "@aligntrue/plugin-contracts": "^0.1.0",
    "@aligntrue/schema": "^0.1.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "yaml": "^2.6.1"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public"
  }
}
